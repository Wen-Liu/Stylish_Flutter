import 'dart:convert';

class Cloth {
  String? image;
  String? name;
  int? price;

  Cloth(this.image, this.price, this.name);

  Cloth.fromJson(Map json) {
    image = json['image'];
    price = json['price'];
  }
}

class ClothList {
  String type;
  List<Cloth> list;

  ClothList(this.type, this.list);
}